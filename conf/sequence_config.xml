			<excfile></excfile>
			<!-- 包含扩展名列表，用*分割 -->
			<extend>.xml</extend>
			<!-- ';'表示输出多个文件,输出为csv文件 -->
			<outfile>FR\xml_new.csv;FR\xml_amd.csv</outfile>
		</file-search>
